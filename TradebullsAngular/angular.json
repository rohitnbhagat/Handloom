{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TradebullsAngular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/tradebulls-angular",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.css",
              "src/assets/vendors/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/vendors/font-awesome/css/font-awesome.min.css",
              "src/assets/vendors/nprogress/nprogress.css",
              "src/assets/vendors/iCheck/skins/flat/green.css",
              "src/assets/vendors/google-code-prettify/bin/prettify.min.css",
              "src/assets/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css",
              "src/assets/vendors/select2/dist/css/select2.min.css",
              "src/assets/vendors/switchery/dist/switchery.min.css",
              "src/assets/vendors/starrr/dist/starrr.css",
              "src/assets/vendors/jqvmap/dist/jqvmap.min.css",
              "src/assets/vendors/bootstrap-daterangepicker/daterangepicker.css",
              "src/assets/vendors/animate.css/animate.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/datatables.net-dt/css/dataTables.dataTables.min.css",
              "node_modules/switchery/standalone/switchery.css",
              "src/assets/build/css/custom.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "src/assets/vendors/jquery/dist/jquery.min.js",
              "src/assets/vendors/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/build/js/custom.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all",
              "baseHref": "/App/"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "TradebullsAngular:build:production"
            },
            "development": {
              "buildTarget": "TradebullsAngular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "TradebullsAngular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.css",
              "src/assets/vendors/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/vendors/font-awesome/css/font-awesome.min.css",
              "src/assets/vendors/nprogress/nprogress.css",
              "src/assets/vendors/iCheck/skins/flat/green.css",
              "src/assets/vendors/google-code-prettify/bin/prettify.min.css",
              "src/assets/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css",
              "src/assets/vendors/select2/dist/css/select2.min.css",
              "src/assets/vendors/switchery/dist/switchery.min.css",
              "src/assets/vendors/starrr/dist/starrr.css",
              "src/assets/vendors/jqvmap/dist/jqvmap.min.css",
              "src/assets/vendors/bootstrap-daterangepicker/daterangepicker.css",
              "src/assets/vendors/animate.css/animate.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/datatables.net-dt/css/dataTables.dataTables.min.css",
              "node_modules/switchery/standalone/switchery.css",
              "src/assets/build/css/custom.css",
              "node_modules/@ckeditor/ckeditor5-build-classic/build/styles.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "src/assets/vendors/jquery/dist/jquery.min.js",
              "src/assets/vendors/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/vendors/fastclick/lib/fastclick.js",
              "src/assets/vendors/nprogress/nprogress.js",
              "src/assets/vendors/Chart.js/dist/Chart.min.js",
              "src/assets/vendors/gauge.js/dist/gauge.min.js",
              "src/assets/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js",
              "src/assets/vendors/iCheck/icheck.min.js",
              "src/assets/vendors/skycons/skycons.js",
              "src/assets/vendors/Flot/jquery.flot.js",
              "src/assets/vendors/Flot/jquery.flot.pie.js",
              "src/assets/vendors/Flot/jquery.flot.time.js",
              "src/assets/vendors/Flot/jquery.flot.stack.js",
              "src/assets/vendors/Flot/jquery.flot.resize.js",
              "src/assets/vendors/flot.orderbars/js/jquery.flot.orderBars.js",
              "src/assets/vendors/flot-spline/js/jquery.flot.spline.min.js",
              "src/assets/vendors/flot.curvedlines/curvedLines.js",
              "src/assets/vendors/DateJS/build/date.js",
              "src/assets/vendors/jqvmap/dist/jquery.vmap.js",
              "src/assets/vendors/jqvmap/dist/maps/jquery.vmap.world.js",
              "src/assets/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js",
              "src/assets/vendors/moment/min/moment.min.js",
              "src/assets/vendors/bootstrap-daterangepicker/daterangepicker.js",
              "src/assets/vendors/bootstrap-daterangepicker/build/js/bootstrap-datetimepicker.min.js",
              "src/assets/vendors/bootstrap-wysiwyg/js/bootstrap-wysiwyg.min.js",
              "src/assets/vendors/jquery.hotkeys/jquery.hotkeys.js",
              "src/assets/vendors/google-code-prettify/src/prettify.js",
              "src/assets/vendors/jquery.tagsinput/src/jquery.tagsinput.js",
              "src/assets/vendors/switchery/dist/switchery.min.js",
              "src/assets/vendors/select2/dist/js/select2.full.min.js",
              "src/assets/vendors/parsleyjs/dist/parsley.min.js",
              "src/assets/vendors/autosize/dist/autosize.min.js",
              "src/assets/vendors/devbridge-autocomplete/dist/jquery.autocomplete.min.js",
              "src/assets/vendors/starrr/dist/starrr.js",
              "src/assets/build/js/custom.js",
              "node_modules/datatables.net/js/dataTables.min.js",
              "node_modules/datatables.net-dt/js/dataTables.dataTables.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}